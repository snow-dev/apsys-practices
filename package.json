{
  "name": "apsys-practice",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.0",
    "axios": "^0.18.0",
    "enzyme": "^3.9.0",
    "express": "^4.16.4",
    "immutable": "^4.0.0-rc.12",
    "prop-types": "^15.7.2",
    "react-autobind": "^1.0.6",
    "react-localize-redux": "^3.5.2",
    "react-redux": "^7.0.2",
    "react-responsive-modal": "^3.6.0",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/resources -o src/resources ",
    "watch-css": "npm run build-css && node-sass-chokidar src/resources -o src/resources --watch --recursive --usePolling",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test",
    "e2e:test": "./node_modules/.bin/testcafe firefox src/tests/__e2e__/",
    "eject": "react-scripts eject",
    "start:testing": "env-cmd .env.development npm run start",
    "build:testing": "env-cmd .env.testing npm run build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "env-cmd": "^8.0.2",
    "enzyme-adapter-react-16": "^1.12.1",
    "node-sass": "^4.11.0",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.5",
    "react-test-renderer": "^16.8.6",
    "redux-devtools-extension": "^2.13.8",
    "redux-mock-store": "^1.5.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/dist/**",
      "!**/vendor/**",
      "!**/server/**",
      "!**/public/**"
    ]
  }
}
